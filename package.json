{
  "name": "treecraft",
  "displayName": "Treecraft - File Structure Generator",
  "description": "Generate nested folders and files based on a simple text input.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "treecraft.createFileTree",
        "title": "Treecraft: Create File Tree from Text",
        "category": "TreeCraft"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "yeoman-environment": "^4.4.1",
    "yeoman-generator": "^7.3.2"
  }
  ,
  "repository": {
    "type": "git",
    "url": "https://github.com/storm12t48/Treecraft.git"
}

}
